{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-azureprojectone"
		},
		"adls_az_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_az'"
		},
		"az_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'az_sql'"
		},
		"adls_az_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsanshproject1.dfs.core.windows.net/"
		},
		"az_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "az-project.database.windows.net"
		},
		"az_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "az-projectdb"
		},
		"az_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminade"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-azureprojectone_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_az_accountKey": {
						"value": "[parameters('adls_az_accountKey')]"
					},
					"az_sql_password": {
						"value": "[parameters('az_sql_password')]"
					},
					"adls_az_properties_typeProperties_url": {
						"value": "[parameters('adls_az_properties_typeProperties_url')]"
					},
					"az_sql_properties_typeProperties_server": {
						"value": "[parameters('az_sql_properties_typeProperties_server')]"
					},
					"az_sql_properties_typeProperties_database": {
						"value": "[parameters('az_sql_properties_typeProperties_database')]"
					},
					"az_sql_properties_typeProperties_userName": {
						"value": "[parameters('az_sql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}